   "source": [
    "# Q-14. Write an SQL query to clone a new table from another table."
   ]
  },
  {
   "cell_type": "raw",
   "id": "e6f5f176",
   "metadata": {},
   "source": [
    "select * into employee1 from worker;\n",
    "\n",
    "select * from employee1;"
